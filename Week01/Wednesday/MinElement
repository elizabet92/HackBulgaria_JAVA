
public class MinEl {
	public static int minEl(int[] array) {
		int min = array[0];
		for(int i=0; i<array.length; i++)
		{
			if(array[i]<min)
			{min = array[i];}
		}
		return min;
	}

	public static void main(String[] args) {
		int[] arr = {1, 2, 3, 0, 9, 5};
		System.out.println(minEl(arr));
	}

}
